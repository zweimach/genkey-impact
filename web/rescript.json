{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "genkey-impact-web",
  "namespace": false,
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true,
    "suffix": ".res.js"
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "reanalyze": {
    "analysis": ["dce", "exception", "termination"]
  },
  "bsc-flags": ["-open RescriptCore"],
  "bs-dependencies": ["@rescript/core", "@rescript/react", "rescript-schema", "rescript-webapi"]
}
