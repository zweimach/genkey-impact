{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "genkey-impact-web",
  "namespace": false,
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "esmodule",
    "in-source": true,
    "suffix": ".res.js"
  },
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "dependencies": ["@rescript/react", "rescript-webapi", "sury"]
}
